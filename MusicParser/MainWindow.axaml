<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="440" d:DesignHeight="450"
        x:Class="MusicParser.MainWindow"
        Title="MusicParser">
	<StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Top" Width="400">
		<StackPanel Orientation="Horizontal">
			<TextBox Text="{Binding Url}" Width="350"></TextBox>
			<Button Name="Parse" Click="ButtonParseClick">Parse</Button>
		</StackPanel>
		<TextBlock Text="{Binding HtmlPage}"></TextBlock>
		<ListBox>
			<ListBoxItem >P1</ListBoxItem>
			<ListBoxItem >P2</ListBoxItem>
			<ListBoxItem >P3</ListBoxItem>
		</ListBox>
	</StackPanel>
</Window>
